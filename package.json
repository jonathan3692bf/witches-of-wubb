{
  "name": "witches-of-wubb",
  "description": "Frontend for Witches of woe",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "test": "vitest --run --reporter verbose",
    "test-filter": "vitest --reporter verbose --testNamePattern",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write --ignore-path .gitignore  --config .prettierrc .",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "postinstall": "cd backend && yarn install",
    "start-backend": "cd backend && yarn start",
    "start-frontend": "vite"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.14",
    "@headlessui/tailwindcss": "^0.1.3",
    "@rollup/plugin-dsv": "^3.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.28.5",
    "autoprefixer": "^10.4.13",
    "classnames": "^2.3.2",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^8.0.3",
    "js-logger": "^1.6.1",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io-client": "^4.6.1",
    "tailwindcss": "^3.2.6",
    "typescript": "5.0.4",
    "vite": "^4.1.0",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.28.5"
  }
}
